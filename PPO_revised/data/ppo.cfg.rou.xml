<?xml version="1.0" encoding="UTF-8"?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">

    <!-- Vehicle types -->
    <vType id="car" accel="2.6" decel="4.5" sigma="0.5" length="5" maxSpeed="25" guiShape="passenger"/>
    <vType id="bike" accel="1.5" decel="3.0" sigma="0.5" length="2" maxSpeed="15" guiShape="bicycle"/>

    <!-- Cars: North approach (-E12) -->
    <flow id="car_N2S" begin="0" end="3600" from="-E12" to="-E14" type="car" vehsPerHour="250"/>
    <flow id="car_N2E" begin="0" end="3600" from="-E12" to="-E13" type="car" vehsPerHour="400"/>
    <flow id="car_N2W" begin="0" end="3600" from="-E12" to="-E15" type="car" vehsPerHour="300"/>

    <!-- Cars: East approach (E13) -->
    <flow id="car_E2W" begin="0" end="3600" from="E13" to="E12" type="car" vehsPerHour="350"/>
    <flow id="car_E2S" begin="0" end="3600" from="E13" to="-E14" type="car" vehsPerHour="300"/>
    <flow id="car_E2N" begin="0" end="3600" from="E13" to="-E15" type="car" vehsPerHour="400"/>

    <!-- Cars: South approach (E14) -->
    <flow id="car_S2N" begin="0" end="3600" from="E14" to="-E15" type="car" vehsPerHour="250"/>
    <flow id="car_S2E" begin="0" end="3600" from="E14" to="E12" type="car" vehsPerHour="300"/>
    <flow id="car_S2W" begin="0" end="3600" from="E14" to="-E13" type="car" vehsPerHour="350"/>

    <!-- Cars: West approach (E15) -->
    <flow id="car_W2E" begin="0" end="3600" from="E15" to="E12" type="car" vehsPerHour="300"/>
    <flow id="car_W2N" begin="0" end="3600" from="E15" to="-E14" type="car" vehsPerHour="250"/>
    <flow id="car_W2S" begin="0" end="3600" from="E15" to="-E13" type="car" vehsPerHour="250"/>

    <!-- Bikes: North approach (-E12) -->
    <flow id="bike_N2E" begin="0" end="3600" from="-E12" to="-E13" type="bike" vehsPerHour="80"/>
    <flow id="bike_N2S" begin="0" end="3600" from="-E12" to="-E14" type="bike" vehsPerHour="60"/>
    <flow id="bike_N2W" begin="0" end="3600" from="-E12" to="-E15" type="bike" vehsPerHour="50"/>

    <!-- Bikes: East approach (E13) -->
    <flow id="bike_E2N" begin="0" end="3600" from="E13" to="E12" type="bike" vehsPerHour="80"/>
    <flow id="bike_E2S" begin="0" end="3600" from="E13" to="-E14" type="bike" vehsPerHour="70"/>
    <flow id="bike_E2W" begin="0" end="3600" from="E13" to="-E15" type="bike" vehsPerHour="60"/>

    <!-- Bikes: South approach (E14) -->
    <flow id="bike_S2E" begin="0" end="3600" from="E14" to="-E15" type="bike" vehsPerHour="80"/>
    <flow id="bike_S2W" begin="0" end="3600" from="E14" to="-E13" type="bike" vehsPerHour="70"/>
    <flow id="bike_S2N" begin="0" end="3600" from="E14" to="E12" type="bike" vehsPerHour="60"/>

    <!-- Bikes: West approach (E15) -->
    <flow id="bike_W2E" begin="0" end="3600" from="E15" to="E12" type="bike" vehsPerHour="50"/>
    <flow id="bike_W2N" begin="0" end="3600" from="E15" to="-E14" type="bike" vehsPerHour="80"/>
    <flow id="bike_W2S" begin="0" end="3600" from="E15" to="-E13" type="bike" vehsPerHour="70"/>
</routes>
